{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "rules": {
    "max-len": 0,
    "no-underscore-dangle": 0,
    "import/no-unresolved": 0,
    "object-curly-newline": 0,
    "no-undef": 0,
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "new-cap": [
      "error",
      {
        "properties": false
      }
    ]
  }
}